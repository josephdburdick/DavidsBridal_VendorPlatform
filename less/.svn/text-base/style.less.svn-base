@textColorSteelBlue:		  #5E748A;	/* same as @colorIconBackground */
@datepickerActiveColor:   @buttonRoseGradientStart;
@heightSearchResult:		  140px;
@fontHeading:				      Georgia, serif;		/* font used for headings */
@fontHelvetica:				    Helvetica, Arial, sans-serif;
@widthHeartSmall:			    13px;				/* width of small heart used for ratings */
