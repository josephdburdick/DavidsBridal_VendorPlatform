@radius:3px;
.rounded (@radius: 3px) {
  border-radius: @radius;
  -ms-border-radius: @radius;
  -webkit-border-radius: @radius;
  -moz-border-radius: @radius;
  -o-border-radius: @radius;
  -khtml-border-radius: @radius;
  //behavior: url(less/PIE.htc);  /* from CSS3PIE */
}  

@textColorRoseHex:    		#B42552;	/* "Rose" color used throughout site design */
.textShadowRoseHex        { text-shadow:0 1px 0px lighten(@textColorRoseHex,50%); }
.btn                      { box-shadow:0 1px 2px rgba(0,0,0,.15); .rounded(@radius);  }
.btn-primary              { position:relative; display:inline-block;color: #FFF;border-color: @buttonRoseGradientStart; #gradient > .vertical(@buttonRoseGradientStart, @buttonRoseGradientEnd );}
@buttonRoseGradientStart:	#BA637D;
@buttonRoseGradientEnd:		#A73754;	/* Starting/ending colors used in rose-colored button gradients */
