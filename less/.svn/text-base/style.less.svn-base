@colorIconBackground:		  #5E748A;			/* background color for button icon images -- same as @textColorSteelBlue */
@colorGrayBorder:			    #BBBBBA;			/* border used to frame various sections */
@widthNavigation:			    620px;
@countNavItems:				    4;					/* number of items in navigation */
